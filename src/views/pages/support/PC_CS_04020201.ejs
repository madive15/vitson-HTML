<% const _src = srcPath; const _public = publicPath; %>
<!DOCTYPE html>
<!--
  [D] vits-scope: 스크롤바 커스텀 범위 제한(운영 전역 scrollbar 충돌 방지)
    - 문서 스크롤 주체가 html/body로 달라질 수 있어 둘 다 클래스 적용
  -->
<html lang="ko" class="vits-scope">
  <%- include(`${_src}/views/layout/head.ejs`, {meta_title: '시국세(전선)-상세'}) %>
  <!-- [D] vits-scope 적용(상단 주석 참고) -->
  <body class="vits-scope">
    <!-- S: Skip Navigation -->
    <a href="#skipNav" class="skip-nav">본문 바로가기</a>
    <!-- E: Skip Navigation -->

    <!-- S: vits-wrapper -->
    <section class="vits-wrapper">
      <%- include(`${_src}/views/layout/header.ejs`) %>

      <!-- S: vits-main -->
      <main id="skipNav" class="vits-main">
        <!-- S: vits-wrap -->
        <div class="vits-wrap">
          <!-- S: vits-grid -->
          <div class="vits-grid">
            <!-- S: vits-grid-main -->
            <div class="vits-grid-main vits-main-support-library">
              <div class="vits-inner inner-column">
                <!-- 1280 컨텐츠 -->
                <div class="vits-grid-area">
                  <!-- S: vits-grid-side-left -->
                  <!-- prettier-ignore -->
                  <%- include(`${_src}/views/layout/lnb/lnb-support.ejs`, {currentMenu: 'library-tax'}) %>
                  <!-- E: vits-grid-side-left -->

                  <div class="vits-grid-main-content main-content-quality">
                    <!-- 메인 컨텐츠 -->

                    <!-- 컨텐츠 헤딩 -->
                    <div class="vits-support-content-headline quality-headline">
                      <h2 class="vits-support-content-title">시국세(전선)</h2>
                    </div>

                    <div class="vits-support-panel quality-panel inner-lg">
                      <!-- S: 상세 보기 -->
                      <!-- prettier-ignore -->
                      <div class="support-detail">
                        <div class="support-detail-header">
                          <h3 class="support-detail-title">강원전선 시국세</h3>
                          <span class="support-detail-date">2027-01-01</span>
                        </div>
                        <div class="support-detail-body">
                          <p class="support-detail-content">
                            # 강원전선 시국세

                            - 업데이트 요청 : MD 전선 담당자
                            
                            게시글 내용이 들어가는 영역입니다.
게시글 내용이 들어가는 영역입니다.
게시글 내용이 들어가는 영역입니다.
게시글 내용이 들어가는 영역입니다.</p>
                        </div>
                        <div class="support-detail-files">
                          <p class="support-detail-files-title">첨부파일</p>
                          <div class="support-file-list">
                            <a href="#" class="vits-btn-md icon-left vits-btn-outline-tertiary" download>
                              <span class="icon"><%- include(`${_src}/views/components/icon.ejs`, { iconName: 'line-download' }) %></span>
                              <span class="text">251209_에스엠씨 12월_국세청세서(26.01.01).pdf</span>
                            </a>
                            <a href="#" class="vits-btn-md icon-left vits-btn-outline-tertiary" download>
                              <span class="icon"><%- include(`${_src}/views/components/icon.ejs`, { iconName: 'line-download' }) %></span>
                              <span class="text">251209_에스엠씨 12월_지방세(26.01.01).pdf</span>
                            </a>
                          </div>
                        </div>
                      </div>
                      <!-- E: 상세 보기 -->
                    </div>

                    <!-- 하단,버튼영역 -->
                    <div class="support-button-area in-support">
                      <!-- prettier-ignore -->
                      <%- include(`${_src}/views/components/button/button.ejs`, { 
                        size: 'xl', 
                        variant: 'outline-secondary', 
                        iconName: 'ic-arrow-left',
                        iconLeft: true,
                        text: '목록으로' }) %>
                    </div>
                  </div>
                </div>
              </div>

              <!-- full wide 컨텐츠 -->
            </div>
            <!-- E: vits-grid-main -->
          </div>
          <!-- E: vits-grid -->
        </div>
        <!-- E: vits-wrap -->
      </main>
      <!-- E: vits-main -->

      <%- include(`${_src}/views/layout/footer.ejs`) %>
    </section>
    <!-- E: vits-wrapper -->

    <%- include(`${_src}/views/layout/footer.links.ejs`) %>

    <script>
      (function () {})();
    </script>
  </body>
</html>
