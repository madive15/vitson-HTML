<% const _src = srcPath; const _public = publicPath; %>
<!-- prettier-ignore -->

<!DOCTYPE html>
<html lang="ko" class="vm-scope">
  <%- include(`${_src}/views/layout-mo/header/head.ejs`, {meta_title: '1:1 문의-상세'}) %>
  <body class="vm-scope">
    <!-- S: Skip Navigation -->
    <a href="#skipNav" class="skip-nav">본문 바로가기</a>
    <!-- E: Skip Navigation -->

    <!-- S: vm-wrapper -->
    <section class="vm-wrapper">
      <!-- S: vm-wrap -->
      <div class="vm-wrap wrap-my-inquiry">
        <!-- S: vm-content-wrap -->
        <div class="vm-content-wrap">
          <!-- prettier-ignore -->
          <%- include(`${_src}/views/layout-mo/header/index.ejs`, { 
            type: 'nav-bar', title: '1:1 문의' 
          }) %>

          <!-- S: vm-main -->
          <main id="skipNav" class="vm-main">
            <div class="vm-main-content main-content-my-inquiry">
              <div class="vm-mypage-inquiry inquiry-detail">
                <div class="vm-mypage-inquiry-detail">
                  <div class="inquiry-private-post">
                    <div class="private-post-head">
                      <div class="private-post-meta">
                        <!-- 
                          [D] 답변대기 : 
                            include(`${_src}/views/components/label/general-label.ejs`, { text: '답변대기', variant: 'gray' }) 
                        -->
                        <%- include(`${_src}/views/components/label/general-label.ejs`, { text: '답변완료', variant:
                        'green' }) %>
                        <p class="head-text text-cate">배송 > 배송일정</p>
                        <p class="head-text text-date">2027-01-01</p>
                      </div>
                      <p class="private-post-title">당일 출고 문의드립니다.</p>
                    </div>
                    <div class="private-post-body">
                      <!-- S: [D] 주문정보 있을 때 -->
                      <a href="#!" class="private-post-order-item">
                        <div class="order-item-thumb">
                          <img src="https://vitsonimg.co.kr/images/productsNew/56/56290.png" alt="" />
                        </div>
                        <p class="order-item-name">
                          <span>[2601000243]</span><span class="name">전산볼트</span><span>외 n건</span>
                        </p>
                      </a>
                      <!-- E: [D] 주문정보 있을 때 -->
                      <div class="private-post-content">당일 출고인데 언제쯤 배송완료 되나요?</div>
                    </div>
                  </div>

                  <!-- 댓글 영역 -->
                  <%- include(`${_src}/views/components-mo/form/post-comment.ejs`) %>
                </div>
              </div>
            </div>
          </main>
          <!-- E: vm-main -->

          <%- include(`${_src}/views/layout-mo/footer/index.ejs`) %>
        </div>
        <!-- E: vm-content-wrap -->

        <!-- S: vm-bottom-group -->
        <!-- prettier-ignore -->
        <%- include(`${_src}/views/layout-mo/bottom-bar/index.ejs`, { 
          type: 'tab-bar', currentTab: 'mypage' 
        }) %>
        <!-- E: vm-bottom-group -->

        <!-- S: 팝업 영역 -->
        <!-- E: 팝업 영역 -->
      </div>
      <!-- E: vm-wrap -->
    </section>
    <!-- E: vm-wrapper -->

    <%- include(`${_src}/views/layout/footer.links.ejs`) %>
  </body>
</html>
