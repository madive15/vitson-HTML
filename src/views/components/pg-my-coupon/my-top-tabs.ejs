<% const _src = srcPath; const _public = publicPath; %>
<!-- prettier-ignore -->
<% const _tabs = typeof tabs !== 'undefined' ? tabs : [ { text: '보유 쿠폰', badge: '22', isActive: true }, { text: '쿠폰 받기', badge: 'N' } ]; %>
<!-- 상단 탭 -->
<div class="vits-mypage-top-tabs" role="tablist">
  <% _tabs.forEach(function(tab, index) { %>
  <button
    type="button"
    class="vits-btn-md vits-my-coupon-tab-btn<%= tab.isActive ? ' is-active' : '' %>"
    aria-selected="<%= tab.isActive ? 'true' : 'false' %>"
    id="tab-<%= index %>"
  >
    <span class="text"><%= tab.text %></span>
    <% if (tab.badge) { %>
    <span class="coupon-badge" aria-label="<%= tab.badge === 'N' ? '신규' : tab.badge + '개' %>"><%= tab.badge %></span>
    <% } %>
  </button>
  <% }); %>
</div>
