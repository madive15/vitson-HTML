<% const _src = srcPath; const _public = publicPath; %>
<!-- prettier-ignore -->

<%
// 마이페이지 메뉴
const gnbMenu = [
  {
    id: 'order',
    label: '주문관리',
    href: '/mypage/order',
    children: [
      { id: 'order-delivery', label: '주문/배송조회', href: '/mypage/order/delivery' },
      { id: 'order-cancel', label: '취소/반품내역', href: '/mypage/order/cancel' },
      { id: 'order-frequent', label: '자주구매', href: '/mypage/order/frequent' },
      { id: 'order-like', label: '좋아요', href: '/mypage/order/like' },
    ],
  },
  {
    id: 'docs',
    label: '서류관리',
    href: '/mypage/docs',
    children: [
      { id: 'docs-tax', label: '세금계산서 조회', href: '/mypage/docs/tax' },
      { id: 'docs-quote', label: '견적서 조회', href: '/mypage/docs/quote' },
      { id: 'docs-bill', label: '청구서 조회', href: '/mypage/docs/bill' },
    ],
  },
  {
    id: 'benefit',
    label: '혜택관리',
    href: '/mypage/benefit',
    children: [
      { id: 'benefit-coupon', label: '쿠폰함', href: '/mypage/benefit/coupon' },
    ],
  },
  {
    id: 'info',
    label: '정보관리',
    href: '/mypage/info',
    children: [
      { id: 'info-member', label: '회원정보 관리', href: '/mypage/info/member' },
      { id: 'info-address', label: '배송지 관리', href: '/mypage/info/address' },
      { id: 'info-pay', label: '비츠온페이 관리', href: '/mypage/info/pay' },
      { id: 'info-postpay', label: '후불관리', href: '/mypage/info/postpay' },
      { id: 'info-qna', label: '1:1 문의', href: '/mypage/info/qna' },
      { id: 'info-bulk', label: '대량구매문의', href: '/mypage/info/bulk' },
      { id: 'info-account', label: '계정등록', href: '/mypage/info/account' },
      { id: 'info-survey', label: '설문조사', href: '/mypage/info/survey' },
    ],
  },
];
%>
 

<%# 마이페이지 전용 좌측 메뉴 %>
<aside class="vits-grid-side vits-grid-side-left vits-side-mypage">
  <h2 class="mypage-sidemenu-title">마이페이지</h2>
  <nav class="mypage-sidemenu" aria-label="마이페이지 메뉴">
    <ul class="mypage-sidemenu-list">
      <% (gnbMenu || []).forEach((item) => { %>
      <li class="mypage-sidemenu-item" data-menu-id="<%= item.id %>">
        <a class="mypage-sidemenu-link" href="<%= item.href %>"><%= item.label %></a>

        <ul class="mypage-sidemenu-sub-list">
          <% (item.children || []).forEach((sub) => { %>
          <li class="mypage-sidemenu-sub-item" data-menu-id="<%= sub.id %>">
            <a class="mypage-sidemenu-link" href="<%= sub.href %>"><%= sub.label %></a>
          </li>
          <% }) %>
        </ul>
      </li>
      <% }) %>
    </ul>
  </nav>
</aside>
